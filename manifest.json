{
  "manifest_version": 2,
  "name": "Easify",
  "version": "0.0.6",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "assets/128.png"
  },

  "background":{
    "scripts": [
      "devJs/devStorage.js",
      "devJs/devTab.js",
      "background.js"
    ]
  },

  "permissions": [
    "storage",
    "tabs"
  ]
}
